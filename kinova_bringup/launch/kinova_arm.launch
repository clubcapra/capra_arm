<launch>

  <arg name="use_urdf" default="false" />
  <arg name="kinova_robotType" default="c2s6s000" />
  <arg name="kinova_robotName" default="arm" />
  <arg name="kinova_robotSerial" default="not_set" />
  <arg name="use_jaco_v1_fingers" default="false" />
  <arg name="feedback_publish_rate" default="0.1" />

  <node name="$(arg kinova_robotName)" ns="ovis" pkg="kinova_driver" type="kinova_arm_driver" output="screen" cwd="node" args="$(arg kinova_robotType)">
    <rosparam file="$(find kinova_bringup)/launch/config/robot_parameters.yaml" command="load" />
    <param name="serial_number" value="$(arg kinova_robotSerial)" />
    <param name="robot_name" value="$(arg kinova_robotName)" />
    <param name="robot_type" value="$(arg kinova_robotType)" />
    <param name="use_jaco_v1_fingers" value="$(arg use_jaco_v1_fingers)" />
    <param name="status_interval_seconds" value="$(arg feedback_publish_rate)" />
    <param name="number_of_degree_per_sec" value="5" />
    <param name="loop_per_command" value="50" />
    <param name="home_position_actuator1" value="16.312500" />
    <param name="home_position_actuator2" value="3.140625" />
    <param name="home_position_actuator3" value="89.953125" />
    <param name="home_position_actuator4" value="331.909088" />
    <param name="home_position_actuator5" value="184.227280" />
    <param name="home_position_actuator6" value="301.977295" />
  </node>

</launch>